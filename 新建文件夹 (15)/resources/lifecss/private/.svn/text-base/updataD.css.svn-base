/*失败弹出框*/
.mark{
	position:fixed;
	_position:absolute;
	left:0;
	top:0;
	width:100%;
	height:100%;
	
}
.fail_box{
	text-align: center;
	position:absolute;
	top:50%;
	left:50%;
	width:382px;
	height:180px;
	margin-top:-90px;
	margin-left:-191px;	
	background: rgba(5,5,5,.5);
	filter: progid:DXImageTransform.Microsoft.Gradient(startColorstr=#88000000,endColorstr=#88000000);
}
.fail_box img{
	margin-top:50px;
}

.fail_box p{
	margin-top:20px;
	font-size:24px;
	color:#fff;
}












