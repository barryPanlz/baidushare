.pay-select{
	margin-left:54px;
}

.pay-select li input[type="radio"]:after{
	display: inline-block;
	width:14px;
	height:14px;
	background:#fff;
	border:1px solid #fff;
	 box-sizing: border-box;
    content: url(../../img/pay-icon1.png);
    line-height: 14px;
    text-align: center;
  
}
.pay-select li input[type="radio"]:checked:after{
	content: url(../../img/pay-icon2.png);
	width:14px;
	height:14px;
}
.pay-btn a{
	display: block;
	width:180px;
	height:40px;
	background: #F3001C;
	line-height: 40px;
	text-align: center;
	background: url(../../img/ping-button.png) no-repeat;
	color:#fff;
	margin:40px 0 0 30px;
	font-size:18px;
	cursor: pointer;
}

