	margin-left:22px;
}
.pay-select .yinlian span{
	margin-left:10px;
}

.pay-select li input{
	margin-right:40px;
	vertical-align: middle;
	cursor:pointer;
}	
.pay-select li input[type="radio"]:after{
	display: inline-block;
	width:14px;
	height:14px;
	background:#fff;
	border:1px solid #fff;
	    box-sizing: border-box;
    content: url(../../img/pay-icon1.png);
    line-height: 14px;
    text-align: center;
  
}
.pay-select li input[type="radio"]:checked:after{
	content: url(../../img/xuanzhongdian.png);
}
.pay-btn a{
	display: block;
	width:180px;
	height:50px;
	background: #F3001C;
	line-height: 50px;
	text-align: center;
	background: url(../../img/lijigoumai-button-ct.png) no-repeat;
	color:#fff;
	margin:40px 0 0 30px;
	font-size:18px;
	cursor: pointer;
	-moz-transition: all .2s ease-in-out .1s;
	-webkit-transition: all .2s ease-in-out .1s;
	transition: all .2s ease-in-out .1s; 

}
.pay-btn a:hover{
	background: url(../../img/lijigoumai-button-hover.png) no-repeat;
}

