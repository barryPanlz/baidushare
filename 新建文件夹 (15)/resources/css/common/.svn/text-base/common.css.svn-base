/*头部用户导航*/
.super_userNavi{
	width: 100%;
	min-width: 1180px;
	height: 36px;
	background: #fff;
}
.userNavi{
	width: 1180px; 
	height: 36px;
	line-height: 36px;
	margin: 0 auto; 
	font-size: 12px;
	color: #666;
}
.Location{ 
	width: 120px; 
	height: 36px;
	float: left;
	cursor: pointer;
	border-bottom: 0;
	border-top: 0;
	border: 1px solid #FFFFFF;
	position: relative;
	padding-left: 5px; 
}
.Location1{
	width: 120px;
	height: 37px;
	float: left;
	cursor: pointer;
