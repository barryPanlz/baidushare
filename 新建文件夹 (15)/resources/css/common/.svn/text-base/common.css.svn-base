	text-align: center;
}
.centerl{
	vertical-align: middle;
}

/*字体大小及颜色定义公用的类*/
.f20c28,.f20cf3,.f20c57{
	font-size:20px;
}
.f16c28,.f16cf3,.f16c57{
	font-size:16px;
}
.f14c28,.f14cf3,.f14c57,.f14c84{
	font-size:14px;
}
.f12c28,.f12cf3,.f12c57,.f12c84{
	font-size:12px;
}
.f20c28,.f16c28,..f14c28,.f12c28{
	color:#282828;
}
.f20cf3,.f12cf3,.f16cf3,.f14cf3{
	color:#F2021d;
}
.f20c57,.f16c57,.f14c57,.f12c57{
	color:#575757;
}
.f14c84,.f12c84{
	color:#848484;
}
.fix:after{
	height:0;
	clear: both;
	content:'';
	display: block;
}
.fix{
	*zoom:1;
}

/*公用部分定义的类结束*/




